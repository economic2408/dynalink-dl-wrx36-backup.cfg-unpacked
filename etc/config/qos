config qos_enable 'qos_enable'
        option Enable '0'
		option New '1'

config qos 'number'
        option MaxShaperEntries '5'
        option ShaperNumberOfEntries '5'
        option MaxQueueEntries '64'
        option QueueNumberOfEntries '2'
        option MaxClassificationEntries '64'
        option ClassificationNumberOfEntries '0'

config qos_config 'classification'
		option maxindex '0'

config shaper 'shaper0'
        option Enable '1'
        option Status '1'
        option Alias ''
        option ShapingBurstSize ''
        option QueueType 'WRR'
        option Interface 'Device.IP.Interface.300'
        option ShapingRate '100'

config shaper 'shaper1'
        option Enable '1'
        option Status '1'
        option Alias ''
        option ShapingBurstSize ''
        option QueueType 'WRR'
        option Interface 'Device.IP.Interface.200'
        option ShapingRate '100'

config shaper 'shaper2'
        option Enable '1'
        option Status '1'
        option Alias ''
        option ShapingBurstSize ''
        option QueueType ''
        option Interface ''
        option ShapingRate ''

config shaper 'shaper3'
        option Enable '1'
        option Status '1'
        option Alias ''
        option ShapingBurstSize ''
        option QueueType ''
        option Interface ''
        option ShapingRate ''

config shaper 'shaper4'
        option Enable '1'
        option Status '1'
        option Alias ''
        option ShapingBurstSize ''
        option QueueType ''
        option Interface ''
        option ShapingRate ''

config queue 'queue1'
		option Alias 'cpe-1'
		option Enable '1'
		option Interface 'Device.IP.Interface.300'
		option Weight '9'
		option SchedulerAlgorithm 'WRR'

